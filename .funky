{"build": "docker compose -f docker/docker-compose.yml -f docker/docker-compose.ui.yml up -d --build \"$@\"", "down": "docker compose -f docker/docker-compose.yml -f docker/docker-compose.ui.yml down --remove-orphans \"$@\"", "logs": "logs() {\n  local cmd=(\n    docker compose\n    -f docker/docker-compose.yml\n    -f docker/docker-compose.ui.yml\n    logs\n    --follow\n  )\n\n  if (( $# > 0 )); then\n    cmd+=(\"$@\")\n  fi\n\n  \"${cmd[@]}\"\n  local status=$?\n\n  if (( status != 0 )); then\n    printf 'logs: command failed with exit code %d\\n' \"$status\" >&2\n  fi\n\n  return $status\n}"}